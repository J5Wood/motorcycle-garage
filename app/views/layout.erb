<!doctype html>
<html>
  <head>
    <title>Motorcycle Garage</title>
  </head>
  <body>
    <table style="width:100%">
      <tr>
        <th><a href="/users/home">Home</a></th>
        <th><a href="/motorcycles">Motorcycles</a></th>
        <th><a href="/brands">Brands</a></th>
        <th><a href="/logout">Logout</a></th>
      </tr>
    </table>
<br>
    <% flash.keys.each do |type| %>
      <div data-alert class="flash <%= type %> alert-box radius" style="color: white; background-color: red;">
        <%= flash[type] %>
        <a href="" class="close">&times;</a>
      </div>
    <% end %>

    <%= yield %>
    
  </body>
</html>
